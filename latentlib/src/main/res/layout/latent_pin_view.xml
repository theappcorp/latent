<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <Button
        android:id="@+id/latent_bt_pin_1"
        style="@style/Latent.Button.Pin.1" />

    <Button
        android:id="@+id/latent_bt_pin_2"
        style="@style/Latent.Button.Pin.2"
        android:layout_toEndOf="@id/latent_bt_pin_1" />

    <Button
        android:id="@+id/latent_bt_pin_3"
        style="@style/Latent.Button.Pin.3"
        android:layout_toEndOf="@id/latent_bt_pin_2" />

    <Button
        android:id="@+id/latent_bt_pin_4"
        style="@style/Latent.Button.Pin.4"
        android:layout_below="@id/latent_bt_pin_1" />

    <Button
        android:id="@+id/latent_bt_pin_5"
        style="@style/Latent.Button.Pin.5"
        android:layout_below="@id/latent_bt_pin_1"
        android:layout_toEndOf="@id/latent_bt_pin_4" />

    <Button
        android:id="@+id/latent_bt_pin_6"
        style="@style/Latent.Button.Pin.6"
        android:layout_below="@id/latent_bt_pin_1"
        android:layout_toEndOf="@id/latent_bt_pin_5" />

    <Button
        android:id="@+id/latent_bt_pin_7"
        style="@style/Latent.Button.Pin.7"
        android:layout_below="@id/latent_bt_pin_4" />

    <Button
        android:id="@+id/latent_bt_pin_8"
        style="@style/Latent.Button.Pin.8"
        android:layout_below="@id/latent_bt_pin_4"
        android:layout_toEndOf="@id/latent_bt_pin_7" />

    <Button
        android:id="@+id/latent_bt_pin_9"
        style="@style/Latent.Button.Pin.9"
        android:layout_below="@id/latent_bt_pin_4"
        android:layout_toEndOf="@id/latent_bt_pin_8" />

    <Button
        android:id="@+id/latent_bt_pin_0"
        style="@style/Latent.Button.Pin.0"
        android:layout_below="@id/latent_bt_pin_8"
        android:layout_toEndOf="@id/latent_bt_pin_7" />

</RelativeLayout>